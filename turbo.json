{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**", "!.next/cache/**", "!.next/dev/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": []
    },
    "lint": {},
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "e2e": {
      "cache": false,
      "outputs": []
    }
  }
}
