{
  "name": "@campreserv/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "build": "nest build",
    "start": "echo '=== SCHEMA CHECK ===' && head -20 prisma/schema.prisma && echo '=== CLEARING PRISMA ===' && rm -rf ../../../node_modules/.prisma ../../../node_modules/@prisma/client && echo '=== REGENERATING ===' && npx prisma generate && npx prisma migrate deploy && echo '=== STARTING ===' && node -r tsconfig-paths/register dist/main.js",
    "test": "jest",
    "test:smoke": "jest --runTestsByPath src/__tests__/observability.e2e.spec.ts src/__tests__/sdk-reservations-smoke.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "prisma db seed",
    "ingest:osm": "ts-node -r tsconfig-paths/register src/scripts/ingest-osm.ts",
    "sandbox:seed": "ts-node prisma/seed-sandbox.ts",
    "openapi:generate": "ts-node --transpile-only -r tsconfig-paths/register src/scripts/generate-openapi.ts",
    "migrate:seasonal-rates": "ts-node -r tsconfig-paths/register src/scripts/migrate-seasonal-rates-to-v2.ts",
    "prisma:studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@campreserv/shared": "workspace:*",
    "@codegenie/serverless-express": "^4.17.1",
    "@google/adk": "^0.2.1",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.7",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@types/express": "^5.0.6",
    "@types/node-fetch": "^2.6.13",
