    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.11",
    "otpauth": "^9.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^7.1.0",
