    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/aws-lambda": "^8.10.159",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
